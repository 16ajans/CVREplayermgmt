{
  "rules": {
    ".read": "auth.token.admin == true",
    "$uid": {
      ".read": true,
      "approved": {
        ".write": "auth.token.admin == true"
      },
      "roster": {
        ".write": "data.parent().child('approved').val() == true"
      },
      "discord": {
        ".write": "$uid === auth.uid"
      }
    }
  }
}